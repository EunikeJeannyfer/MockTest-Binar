          "id": {
            "type": "integer",
            "format": "int32",
            "example": 10
          },
          "nama": {
            "type": "string",
            "example": "John"
          },
          "email": {
            "type": "string",
            "example": "john@email.com"
          },
          "password": {
            "type": "string",
            "example": "12345"
          }
        },
        "xml": {
          "name": "user"
        }
      },
      "ToDo": {
        "type": "object",
        "required": [
          "id_user",
          "judul",
          "deskripsi",
          "tanggal"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "example": 10
          },
          "id_user": {
            "type": "integer",
            "format": "int32",
            "example": 198772
          },
          "judul": {
            "type": "string",
            "example": "Ulangan"
          },
          "deskripsi": {
            "type": "string",
            "example": 2501097342338853
          },
          "createAt": {
            "type": "string",
            "format": "datetime",
            "example": "2019-10-10T00:00:00.000Z"
          },
          "tanggal": {
            "type": "string",
            "format": "datetime",
            "example": "2023-10-1"
          },
          "status": {
            "type": "string",
            "format": "datetime",
            "example": "2023-10-1"
          }
        },
        "xml": {
          "name": "todo"
        }
      }
    },
